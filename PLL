#include <lpc214X.h>

void set_pll(){
	PLL0CON = 0X01;
	PLL0CFG = 0X24;
	PLL0FEED = 0XAA;
	PLL0FEED = 0X55;
	while((PLL0STAT & (1<<10)) == 0);
	PLL0CON = 0X03;
	PLL0FEED = 0XAA;
	PLL0FEED = 0X55;
		
}	

int main(){
	set_pll();
	IODIR0 |= (1<<10);
	while(1){
		IOSET0 = (1<<10);
		IOCLR0 = (1<<10);
	}	
}	
