clc;
t=0:0.1:pi;
y=sin(2*pi*t);
subplot(3,2,1);
plot(t,y);

clc;
t=0:0.1:pi;
y=cos(2*pi*t);
subplot(3,2,2);
plot(t,y);

clc;
t=0:5;
stem(t,t);
subplot(3,2,3);
plot(t,t);

clc;
a=0.4;
t=0:0.1:10;
y=exp(a*t);
subplot(3,2,4);
plot(t,y);

clc;
t=0:1:5;
y1=ones(1,6);
subplot(3,2,5);
stem(t,y1);

clc;t=-5:5;
y=[zeros(1,5),ones(1,1),zeros(1,5)];
subplot(3,2,6);
stem(t,y);
